parameters: [
{
name: "dataset_id",
in: "path",
required: true,
type: "string"
}
],
get: {
operationId: "GET_api-datasets-dataset_id",
description: "<p>Get Nanopub dataset metadata only - cannot export dataset with this endpoint.</p> <p>A dataset is a virtual collection of Nanopubs that has been uploaded in a set either as a BEL Script, XBEL file or an array of BEL Nanopubs. You can only upload a dataset, download the list of datasets, export a dataset in the different file formats or delete a dataset. You cannot change individual Nanopubs in a dataset without downloading the dataset, deleting the prior dataset and then re-POSTing the new dataset.</p> ",
parameters: [
{
name: "Authorization",
in: "header",
required: false,
type: "string",
default: "Bearer <<apiKey>>"
}
],
responses: {
200: {
description: "",
schema: {
type: "object",
properties: {
dataset: {
type: "object",
properties: {
identifier: {
type: "string"
},
title: {
type: "string"
},
description: {
type: "string"
},
