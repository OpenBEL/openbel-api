    put:
      description: |
        Replaces an Evidence resource identified by *{identifier}*.

        **JSON Schema**
        - [evidence_resource](schemas/evidence_resource.schema.json)
        - [evidence](schemas/evidence.schema.json)
      body:
        application/json:
          example: |
            {
              "evidence": {
                "bel_statement": "p(HGNC:MAPK1,pmod(P,T)) => kin(p(HGNC:MAPK1))",
                "citation": {
                  "type": "PubMed",
                  "name": "J Biol Chem. 1997 Jul 4;272(27):16917-23.",
                  "id": "9202001"
